{
  "name": "rippling-medicinals-backend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@aws-amplify/api": "^1.1.3",
    "@aws-amplify/pubsub": "^1.1.2",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.4",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-linkedin-oauth2": "^2.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^7.12.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-scripts": "3.2.0",
    "sequelize": "^5.15.1"
  },
  "scripts": {
    "db:init": "dropdb --if-exists ripplingmedicinals_db && createdb ripplingmedicinals_db && npm run db:reset && npm run db:seed",
    "db:reset": "node ./database/resetDb.js",
    "db:seed": "node ./database/seedDb.js",
    "db:go": "npm run db:reset && npm run db:seed",
    "start": "npx nodemon server.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
